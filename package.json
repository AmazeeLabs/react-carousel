    "react": ">0.14.0 || >15.0.0",
    "react-dom": ">0.14.0 || >15.0.0"
  },
  "keywords": [
    "react",
    "carousel",
    "react-carousel",
    "react-component",
    "component",
    "image",
    "image-gallery",
    "image-slider",
    "responsive",
    "gallery",
    "rwd"
  ],
