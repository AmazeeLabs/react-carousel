{
  "name": "@brainhubeu/react-carousel",
  "version": "1.10.16",
  "description": "Carousel component for React",
  "engines": {
    "npm": ">=6.8.0"
  },
  "files": [
    "lib/*"
  ],
  "scripts": {
    "lint": "esw webpack.config.* src tools test features --color",
    "clean-dist": "rm -rf ./lib && mkdir lib",
    "prebuild": "npm run clean-dist",
    "build": "node tools/build.js",
    "selenium-start": "selenium-standalone start",
    "selenium-install": "selenium-standalone install",
    "test": "npm run test-unit && npm run test-regression",
    "test-unit": "mocha tools/testSetup.js \"./test/unit/*.js\"",
    "test-regression": "gemini test ./test/regression/*js",
    "test-regression-update": "gemini update ./test/regression/*js",
    "test:coverage": "NODE_PATH=example babel-node ./node_modules/.bin/isparta cover _mocha -- --require ./tools/testSetup.js \"./test/unit/*.js\" && open coverage/lcov-report/index.html",
    "test:coverage:ci": "NODE_PATH=example babel-node ./node_modules/.bin/isparta cover _mocha --report lcovonly -- --require ./tools/testSetup.js \"./test/unit/*.js\"",
    "precommit": "npm run lint"
  },
  "author": "Brainhub",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^9.4.9",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-enzyme": "^0.8.0",
    "chalk": "^2.4.2",
    "classnames": "^2.2.6",
    "coveralls": "^3.0.3",
    "css-loader": "^2.1.0",
    "cucumber": "^5.1.0",
    "enzyme": "^3.9.0",
    "eslint": "^5.14.1",
    "eslint-config-brainhub": "^1.10.0",
    "eslint-watch": "^4.0.2",
    "express": "^4.16.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^3.0.1",
    "gemini": "^7.3.0",
    "html-reporter": "^3.6.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "isparta": "^4.1.1",
    "istanbul": "^0.4.5",
    "jsdom": "^13.2.0",
    "json-loader": "^0.5.7",
    "lodash": "^4.17.11",
    "mocha": "^6.0.2",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.8.3",
    "react-fa": "^5.0.0",
    "react-redux": "^6.0.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.8",
    "react-syntax-highlighter": "^10.1.3",
