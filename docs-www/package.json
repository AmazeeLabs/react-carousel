    "lint": "eslint --ext .jsx,.js .",
    "lint:autofix": "eslint --ext .jsx,.js . --fix"
  },
  "dependencies": {
    "@brainhubeu/gatsby-docs-kit": "^1.0.3",
    "@brainhubeu/react-carousel": "^1.10.14",
    "gatsby": "^2.1.19",
    "react-fa": "^5.0.0"
  },
  "devDependencies": {
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "eslint": "^5.14.1",
    "eslint-config-brainhub": "^1.10.0",
    "gh-pages": "^2.0.1",
    "react": "^16.8.3",
    "react-dom": "^16.8.3"
  }
}
